{
  "compilerOptions": {
    "outDir": "./",
    "moduleResolution": "node",
    "target": "esnext",
    "declaration": true,
    "noImplicitAny": false,
    "downlevelIteration": true,
    "sourceMap": true,
    "noImplicitThis": true,
    "alwaysStrict": true,
    "skipLibCheck": true,
    "noUnusedLocals": true,
    "noUnusedParameters": false,
    "jsx": "react",
    "allowSyntheticDefaultImports": true,
    "lib": ["dom", "es5", "es2015.collection", "es2015.iterable"]
  },
  "include": ["src"],
  "exclude": []
}
